                        <a href="javascript:void(0);"><img src="assets/img/icons/time.svg" alt="img"><span> Report</span> <span class="menu-arrow"></span></a>
                        <ul>
                           <li><a asp-page="/salesreport">Invoice Report</a></li>
                        </ul>
                    </li>
                    <li class="submenu">
                        <a href="javascript:void(0);"><img src="assets/img/icons/user2.svg" alt="img"><span> Users</span> <span class="menu-arrow"></span></a>
                        <ul>
                            <li><a asp-page="/addemployee">Add User </a></li>
                            <li><a asp-page="/assignuser">Assign Branch/Organization</a></li>
