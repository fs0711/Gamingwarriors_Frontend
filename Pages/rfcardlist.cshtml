                                        <tr>
                                            @* <td>@item.Id</td> *@
                                            <td>@item.Card_id</td>
                                            <td>@item.Status</td>
                                            <td>@item.Organization</td>
                                            <td>@item.Branch</td>
                                        </tr>
                                    }
                                </tbody>
                            </table>
                        </div>
                    </div>
